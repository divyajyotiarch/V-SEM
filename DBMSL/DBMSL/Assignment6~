SQL> create table n_emp
  2  (
  3  emp_id number(10) not null,
  4  emp_name varchar2(20),
  5  designation varchar2(20) 
  6  );

Table created.

SQL> create table o_emp
  2  (
  3  emp_id number(10) not null,
  4   emp_name varchar2(20),
  5   designation varchar2(20) 
  6  );

Table created.

SQL>  insert all
  2   into o_emp (eid,ename,epost) values (3181,'jim','MD')
  3  into o_emp (eid,ename,epost) values  (3178,'tejas','JS')
  4   into o_emp (eid,ename,epost) values  (3182,'joy','JS')
  5  select * from dual;

3 rows created.

SQL> insert all
  2  into n_emp (emp_id,emp_name,designation) values (1,'bob','MD')
  3  into n_emp (emp_id,emp_name,designation) values (2,'kyle','Principal')
  4  into n_emp (emp_id,emp_name,designation) values (3,'rue','AS')
  5  select * from dual;

3 rows created.


declare
cno number(10);
cursor c(id number) is select * from o_emp where eid=id;
begin 
open c;
loop
fetch c() into cno;
exit when c%NOTFOUND;
insert into n_emp (srno,emp_id,emp_name,post) values (seq.nextval,cno.eid,cno.ename,cno.epost);
end if;
end loop;
end;
/
